{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/webhook/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ],
    "env": {
        "DISCORD_TOKEN": "",
        "MACRO_1": "1461505893794582691",
        "MACRO_2": "1461517312195559530",
        "MACRO_3": "1461517322652221555",
        "WEBSITE_URL": "@website_url"
    }
}
